    },
    "homepage": "https://github.com/developmentseed/raster-uploader#readme",
    "dependencies": {
        "@openaddresses/batch-generic": "^8.0.0",
        "@openaddresses/batch-schema": "^4.0.0",
        "@turf/bbox": "^6.5.0",
