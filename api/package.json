        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "busboy": "^1.6.0",
        "connect-history-api-fallback": "^2.0.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "express-minify": "^1.0.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^2.0.0",
